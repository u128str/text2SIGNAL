{
  "comment": "KPSD000260 Cash collection for sales (invoices)",
  "bpmn_model": "Cash collection sales (invoices)",
  "source_data": [
    {
      "ppi_id": "KPSD000260",
      "type": "main",
      "filter": "VBTYP eq 'M'"
    }
  ],
  "version": "1",
  "process_name": "KPSD000260_01",
  "event_definitions": {
    "Prepare invoice": {
      "pattern": "ERDAT != @initial",
      "timestamp": "ERDAT",
      "timestamp_offset_minutes": 10,
      "id": "EVENT_10",
      "variants": {
        "Prepare delivery-related invoice": {
          "pattern": "FKTYP == 'L'",
          "id": "10"
        },
        "Prepare order-related invoice": {
          "pattern": "FKTYP == 'A'",
          "id": "20"
        },
        "Prepare periodic billing invoice": {
          "pattern": "FKTYP == 'D'",
          "id": "30"
        },
        "Prepare invoicing via billing interface": {
          "pattern": "FKTYP == 'X'",
          "id": "40"
        }
      },
      "include_other_variants": "Prepare invoices - others"
    },
    "Create invoice": {
      "pattern": "CPUDT != @initial",
      "timestamp": "CPUDT",
      "timestamp_offset_minutes": 40,
      "id": "EVENT_20",
      "variants": {
        "Create invoice manually via single transaction VF01": {
          "pattern": "TCODE == 'VF01' and USTYP == 'A'",
          "id": "10"
        },
        "Create invoice via manual billing due processing": {
          "pattern": "TCODE == 'VF04'",
          "id": "20"
        },
        "Create invoice automatically": {
          "pattern": "TCODE == '' or (TCODE == 'VF01' or TCODE == 'VF02') and (USTYP ==  'B' or USTYP == 'C')",
          "id": "30"
        },
        "Create invoice via custom transaction": {
          "pattern": "TCODE.str.startswith('Z') or TCODE.str.startswith('Y') or TCODE.str.startswith('/')",
          "id": "40"
        },
        "Create invoice automatically from shipment": {
          "pattern": "TCODE == 'VTFAKT'",
          "id": "50"
        }
      },
      "include_other_variants": "Create invoice - others"
    },
    "Post invoice to FI": {
      "pattern": "CPUDT != @initial",
      "timestamp": "CPUDT",
      "timestamp_offset_minutes": 80,
      "id": "EVENT_30",
      "variants": {
        "Post invoice automatically to FI": {
          "pattern": "TCODE != 'VFX3' and TCODE != 'VF02' and RFBSK == 'C'",
          "id": "10"
        },
        "Post invoice to FI after block": {
          "pattern": "RFBSK == 'A'",
          "id": "20"
        },
        "Post invoice manually to FI after error resolution via VFX3": {
          "pattern": "TCODE == 'VFX3' or TCODE == 'VF02'",
          "id": "30"
        }
      },
      "include_other_variants": "Post invoice - others"
    },
    "Dun invoice": {
      "pattern": "MADAT_B != @initial",
      "timestamp": "MADAT_B",
      "timestamp_offset_minutes": 120,
      "id": "EVENT_40"
    },
    "Clear FI-AR items": {
      "pattern": "AUGDT != @initial",
      "timestamp": "AUGDT",
      "timestamp_offset_minutes": 160,
      "id": "EVENT_50",
      "variants": {
        "Clear FI-AR item via payment run F110": {
          "pattern": "TCODE_AUGBL == 'F110' or TCODE_AUGBL == ''",
          "id": "10"
        },
        "Clear FI-AR item via account maintenance transaction FB1D": {
          "pattern": "TCODE_AUGBL.str.startswith('FB1D')",
          "id": "20"
        },
        "Clear FI-AR item via manual payment": {
          "pattern": "TCODE_AUGBL.str.startswith('FBZ')",
          "id": "30"
        },
        "Clear FI-AR item via transaction FB05": {
          "pattern": "TCODE_AUGBL == 'FB05'",
          "id": "40"
        },
        "Clear FI-AR item via reversal": {
          "pattern": "TCODE_AUGBL == 'FB08' or TCODE_AUGBL == 'VF11'",
          "id": "50"
        }
      },
      "include_other_variants": "Clear FI-AR item - others"
    }
  }
}