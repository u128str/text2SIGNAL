{
  "comment": "KFFI001861 - Asset master creation (with depreciation) to deactivation",
  "bpmn_model": "Asset master creation (with depreciation) to deactivation",
  "source_data": [
    {
      "ppi_id": "KFFI001861",
      "type": "main"
    }
  ],
  "process_name": "KFFI001861_01",
  "version": "1",
  "event_definitions": {
    "Create asset master": {
      "pattern": "ERDAT != @initial",
      "timestamp": "ERDAT",
      "timestamp_offset_minutes": 20,
      "id": "EVENT_10"
    },
    "Capitalize asset": {
      "pattern": "AKTIV != @initial",
      "timestamp": "AKTIV",
      "timestamp_offset_minutes": 40,
      "id": "EVENT_20"
    },
    "Enter asset acquisition": {
      "pattern": "CPUDT_ACQUI != @initial",
      "timestamp": "CPUDT_ACQUI",
      "timestamp_offset_minutes": 60,
      "id": "EVENT_30",
      "variants": {
        "Enter asset acquisition using Controlling settlement": {
          "pattern": "VORGN_ACQUI == 'ZUCO' and not (TCODE_ACQUI.str.startswith('Z') or TCODE_ACQUI.str.startswith('Y') or TCODE_ACQUI.str.startswith('/'))",
          "id": "10"
        },
        "Enter asset acquisition using purchase order": {
          "pattern": "EBELN_ACQUI != ''",
          "id": "20"
        },
        "Enter asset acquisition using accounting posting": {
          "pattern": "VORGN_ACQUI == 'ZUGA' and TCODE_ACQUI.str.startswith('FB')",
          "id": "30"
        },
        "Enter asset acquisition using clearing account": {
          "pattern": "VORGN_ACQUI == 'ZUGA' and TCODE_ACQUI == 'ABZON'",
          "id": "40"
        },
        "Enter asset acquisition using a custom transaction": {
          "pattern": "VORGN_ACQUI == 'ZUCO' and (TCODE_ACQUI.str.startswith('Z') or TCODE_ACQUI.str.startswith('Y') or TCODE_ACQUI.str.startswith('/'))",
          "id": "50"
        },
        "Enter asset acquisition using internal asset transfer": {
          "pattern": "VORGN_ACQUI == 'UMZI'",
          "id": "60"
        },
        "Enter asset acquisition using intercompany asset transfer": {
          "pattern": "VORGN_ACQUI == 'UMAI'",
          "id": "70"
        }
      },
      "include_other_variants": "Enter asset acquisition - others"
    },
    "Retire asset": {
      "pattern": "DEAKT != @initial",
      "timestamp": "DEAKT",
      "timestamp_offset_minutes": 80,
      "id": "EVENT_40",
      "variants": {
        "Retire asset from sale with customer": {
          "pattern": "VORGN_RETIRE == 'ABGA' and TCODE_RETIRE == 'F-92'",
          "id": "10"
        },
        "Retire asset by scrapping": {
          "pattern": "VORGN_RETIRE == 'ABGA' and TCODE_RETIRE == 'ABAVN'",
          "id": "20"
        },
        "Retire asset from sale without customer": {
          "pattern": "VORGN_RETIRE == 'ABGA' and TCODE_RETIRE == 'ABAON'",
          "id": "40"
        },
        "Retire asset via custom transaction": {
          "pattern": "VORGN_RETIRE == 'ABGA' and (TCODE_RETIRE.str.startswith('Z') or TCODE_RETIRE.str.startswith('Y') or TCODE_RETIRE.str.startswith('/'))",
          "id": "50"
        },
        "Retire asset using intercompany asset transfer": {
          "pattern": "VORGN_RETIRE == 'UMAI'",
          "id": "60"
        },
        "Retire asset using internal asset transfer": {
          "pattern": "VORGN_RETIRE == 'UMZI'",
          "id": "70"
        }
      },
      "include_other_variants": "Retire asset - others"
    }
  }
}