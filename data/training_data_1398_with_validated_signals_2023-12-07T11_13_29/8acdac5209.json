  "name": "Late Payment Rate ",
  "query": "SELECT\n  COUNT(case_id) FILTER (\n    WHERE\n      (\n        SELECT\n          COUNT(event_name)\n        WHERE\n          (\n            \"InvoiceAccountingClearingDate\" > \"InvoiceDueDate\"\n          )\n      ) > 0\n  ) / COUNT(case_id) FILTER (\n    WHERE\n      (\n        SELECT\n          BOOL_OR(event_name IN ('Clear Invoice'))\n      )\n  ) * 100 AS \"Late Payment Rate\"\nFROM\n  \"defaultview-383\"",
